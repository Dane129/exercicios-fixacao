algoritmo "semnome"
// Função :
// Autor :
// Data : 14/10/2023
// Seção de Declarações 
var
gasolina, valor, valorfinal: real
inicio
// Seção de Comandos
escreva("Deseja botar quanto de gasolina? ")
leia(gasolina)
valor <- 5.82
valorfinal <- gasolina / valor
escreva("Você conseguirá botar", valorfinal:6:2, " litros de gasolina")
fimalgoritmo
